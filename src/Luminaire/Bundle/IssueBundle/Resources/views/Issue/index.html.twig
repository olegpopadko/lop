{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set pageTitle = 'luminaire.issue.entity_plural_label'|trans %}
{% set gridName = 'issues-grid' %}

{% block navButtons %}
    {% include 'OroImportExportBundle:ImportExport:buttons.html.twig' with {
        entity_class: entity_class,
        exportProcessor: 'luminaire_issue',
        importProcessor: 'luminaire_issue.add_or_replace',
        importTitle: 'luminaire.issue.import'|trans,
        dataGridName: gridName
    } %}

    {#{% if resource_granted('luminaire_issue_create') %}#}
        <div class="btn-group">
            {{ UI.addButton({
                'path': path('luminaire_issue_create'),
                'entity_label': 'luminaire.issue.entity_label'|trans
            }) }}
        </div>
    {#{% endif %}#}
{% endblock %}
